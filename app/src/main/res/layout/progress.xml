<?xml version="1.0" encoding="utf-8"?><androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    android:layout_width="100dp"    android:background="#00000000"    android:layout_height="100dp">    <com.google.android.material.card.MaterialCardView        android:layout_width="70dp"        android:layout_height="70dp"        app:cardCornerRadius="100dp"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent">        <ProgressBar            android:id="@+id/progressBar"            style="?android:attr/progressBarStyle"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_gravity="center" />    </com.google.android.material.card.MaterialCardView></androidx.constraintlayout.widget.ConstraintLayout>